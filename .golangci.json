{
	"formatters": {
		"enable": ["gofumpt"],
		"exclusions": {
			"generated": "strict",
			"paths": [],
			"warn-unused": false
		},
		"settings": {
			"gofumpt": {
				"extra-rules": true
			}
		}
	},
	"issues": {
		"fix": false
	},
	"linters": {
		"enable": [
			"govet",
			"staticcheck",
			"errcheck",
			"ineffassign",
			"unused",
			"errorlint",
			"gocritic",
			"misspell",
			"bodyclose",
			"revive",
			"prealloc",
			"unparam",
			"paralleltest",
			"goconst",
			"importas",
			"wsl_v5",
			"gosec",
			"nolintlint",
			"noctx",
			"exhaustive",
			"nilnil",
			"makezero",
			"forcetypeassert",
			"nilerr",
			"gocognit",
			"nestif",
			"predeclared",
			"thelper",
			"copyloopvar",
			"intrange",
			"unconvert",
			"wastedassign",
			"whitespace",
			"err113",
			"testpackage",
			"gochecknoglobals",
			"gochecknoinits",
			"funlen"
		]
	},
	"output": {
		"show-stats": true
	},
	"run": {
		"timeout": "5m"
	},
	"version": "2"
}
